{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "plugins": [
    "react",
    "jest"
  ],
  "rules": {
    "semi": [
      1,
      "never"
    ],
    "quotes": [
      1,
      "single"
    ],
    "no-console": 1
  }
}